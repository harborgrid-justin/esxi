{
  "name": "@harborgrid/accessibility-documents",
  "version": "1.0.0",
  "description": "Enterprise document accessibility checker for PDF/UA, EPUB, and Office formats",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "accessibility",
    "a11y",
    "PDF/UA",
    "WCAG",
    "EPUB",
    "document",
    "compliance",
    "office",
    "remediation"
  ],
  "author": "HarborGrid",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^3.11.174",
    "mammoth": "^1.6.0",
    "xlsx": "^0.18.5",
    "jszip": "^3.10.1",
    "cheerio": "^1.0.0-rc.12",
    "xml2js": "^0.6.2",
    "axe-core": "^4.8.3",
    "file-type": "^18.7.0",
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/mime-types": "^2.1.4",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
