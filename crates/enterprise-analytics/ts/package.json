{
  "name": "@harborgrid/enterprise-analytics",
  "version": "0.4.0",
  "description": "Enterprise Analytics & Business Intelligence Dashboard Platform",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.js"
    },
    "./query": {
      "types": "./dist/query/index.d.ts",
      "import": "./dist/query/index.mjs",
      "require": "./dist/query/index.js"
    },
    "./visualization": {
      "types": "./dist/visualization/index.d.ts",
      "import": "./dist/visualization/index.mjs",
      "require": "./dist/visualization/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.mjs",
      "require": "./dist/components/index.js"
    },
    "./services": {
      "types": "./dist/services/index.d.ts",
      "import": "./dist/services/index.mjs",
      "require": "./dist/services/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "keywords": [
    "analytics",
    "business-intelligence",
    "dashboard",
    "data-visualization",
    "charts",
    "d3",
    "olap",
    "bi",
    "enterprise"
  ],
  "author": "HarborGrid",
  "license": "MIT",
  "dependencies": {
    "d3": "^7.8.5",
    "d3-scale": "^4.0.2",
    "d3-shape": "^3.2.0",
    "d3-array": "^3.2.4",
    "d3-hierarchy": "^3.1.2",
    "d3-geo": "^3.1.0",
    "d3-sankey": "^0.12.3",
    "date-fns": "^3.0.6",
    "zod": "^3.22.4",
    "sql.js": "^1.10.2",
    "dexie": "^3.2.4",
    "papaparse": "^5.4.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-grid-layout": "^1.4.4",
    "recharts": "^2.10.3",
    "pdfmake": "^0.2.9",
    "xlsx": "^0.18.5"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/d3": "^7.4.3",
    "@types/d3-scale": "^4.0.8",
    "@types/d3-shape": "^3.1.6",
    "@types/d3-array": "^3.2.1",
    "@types/d3-hierarchy": "^3.1.6",
    "@types/d3-geo": "^3.1.0",
    "@types/d3-sankey": "^0.12.4",
    "@types/papaparse": "^5.3.14",
    "@types/pdfmake": "^0.2.9",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitest/ui": "^1.1.3",
    "eslint": "^8.56.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
}
