  "description": "Enterprise-grade CAD/Vector Editor with GPU acceleration and parametric constraints",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "dev": "tsc --watch",
    "test": "jest",
