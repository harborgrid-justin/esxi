{
  "name": "@enterprise/compression",
  "version": "1.0.0",
  "description": "Enterprise-grade compression and data optimization engine with multi-algorithm support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "compression",
    "optimization",
    "lz4",
    "zstd",
    "brotli",
    "image-optimization",
    "webp",
    "avif",
    "caching",
    "cdn",
    "enterprise"
  ],
  "author": "Enterprise Development Team",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "lz4": "^0.6.5",
    "zstd-codec": "^0.1.5",
    "brotli": "^1.3.3",
    "sharp": "^0.33.0",
    "pako": "^2.1.0",
    "lru-cache": "^10.0.1",
    "ioredis": "^5.3.2",
    "msgpack-lite": "^0.1.26"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/node": "^20.0.0",
    "@types/pako": "^2.0.0",
    "@types/lru-cache": "^7.10.10",
    "typescript": "^5.3.0",
    "eslint": "^8.50.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "prettier": "^3.0.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
