{
  "name": "@harborgrid/enterprise-security",
  "version": "0.4.0",
  "description": "Enterprise-grade Security & Compliance System with authentication, authorization, encryption, and compliance frameworks",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.js"
    },
    "./auth": {
      "types": "./dist/auth/index.d.ts",
      "import": "./dist/auth/index.mjs",
      "require": "./dist/auth/index.js"
    },
    "./authz": {
      "types": "./dist/authz/index.d.ts",
      "import": "./dist/authz/index.mjs",
      "require": "./dist/authz/index.js"
    },
    "./encryption": {
      "types": "./dist/encryption/index.d.ts",
      "import": "./dist/encryption/index.mjs",
      "require": "./dist/encryption/index.js"
    },
    "./compliance": {
      "types": "./dist/compliance/index.d.ts",
      "import": "./dist/compliance/index.mjs",
      "require": "./dist/compliance/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.mjs",
      "require": "./dist/components/index.js"
    },
    "./services": {
      "types": "./dist/services/index.d.ts",
      "import": "./dist/services/index.mjs",
      "require": "./dist/services/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "keywords": [
    "security",
    "compliance",
    "authentication",
    "authorization",
    "encryption",
    "soc2",
    "hipaa",
    "gdpr",
    "pci-dss",
    "rbac",
    "abac",
    "enterprise",
    "saas"
  ],
  "author": "HarborGrid",
  "license": "MIT",
  "dependencies": {
    "zod": "^3.22.4",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "node-forge": "^1.3.1",
    "nanoid": "^5.0.4",
    "date-fns": "^3.0.6"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitest/ui": "^1.1.3",
    "eslint": "^8.56.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
}
