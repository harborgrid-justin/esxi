{
  "name": "@esxi/enterprise-bridge",
  "version": "0.5.0",
  "description": "TypeScript-Rust WASM Bridge for $983M Enterprise SaaS Platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "wasm"
  ],
  "scripts": {
    "build": "tsc && npm run build:wasm",
    "build:wasm": "cd ../../crates/meridian-wasm-bridge && wasm-pack build --target web --out-dir ../../packages/enterprise-bridge/wasm",
    "build:dev": "tsc && npm run build:wasm:dev",
    "build:wasm:dev": "cd ../../crates/meridian-wasm-bridge && wasm-pack build --target web --dev --out-dir ../../packages/enterprise-bridge/wasm",
    "watch": "tsc --watch",
    "clean": "rm -rf dist wasm",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "wasm",
    "webassembly",
    "rust",
    "typescript",
    "bridge",
    "enterprise",
    "cad",
    "compression",
    "collaboration",
    "security",
    "query-optimization",
    "high-performance"
  ],
  "author": "ESXI Enterprise",
  "license": "MIT",
  "dependencies": {
    "comlink": "^4.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/harborgrid-justin/esxi.git",
    "directory": "packages/enterprise-bridge"
  },
  "publishConfig": {
    "access": "public"
  }
}
